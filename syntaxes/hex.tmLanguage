<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>scopeName</key>
        <string>text.hex</string>

        <key>fileTypes</key>
        <array>
            <string>hex</string>
            <string>HEX</string>
        </array>

        <key>name</key>
        <string>Intel Hex format</string>

        <key>patterns</key>
