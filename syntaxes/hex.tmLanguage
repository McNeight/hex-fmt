            <dict>
                <key>match</key>
                <string>(:)([0-9A-Fa-f]{2})([0-9A-Fa-f]{4})([0-9A-Fa-f]{2})([0-9A-Fa-f]*?)([0-9A-Fa-f]{2})$</string>
                <key>captures</key>
                <dict>
                    <key>1</key>
                    <dict>
                        <key>name</key>
                        <string>keyword.operator</string>
                    </dict>
                    <key>2</key>
                    <dict>
                        <key>name</key>
                        <string>constant.numeric</string>
                    </dict>
                    <key>3</key>
                    <dict>
                        <key>name</key>
                        <string>variable.parameter</string>
                    </dict>
                    <key>4</key>
                    <dict>
                        <key>name</key>
                        <string>entity.name.type</string>
                    </dict>
                    <key>6</key>
                    <dict>
                        <key>name</key>
                        <string>markup.bold</string>
                    </dict>
                </dict>
            </dict>
        </array>
    </dict>
</plist>